{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "webrtc.html", "use": "@vercel/static" },
    { "src": "connected.html", "use": "@vercel/static" },
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/webrtc", "dest": "/webrtc.html" },
    { "src": "/connected", "dest": "/connected.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
